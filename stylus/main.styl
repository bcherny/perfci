*
	border 0
	margin 0
	padding 0

body
	font 16px/22px Consolas, "Liberation Mono", Courier, monospace

.null
	font-weight bold

.value
	color #00769B

.number
	color #FF0070

#json
	clear both
	white-space pre

#count
	background orange
	clear both
	float left
	padding 2px 10px

#chart
	bottom 0
	left 0
	position absolute
	right 0
	top 50%